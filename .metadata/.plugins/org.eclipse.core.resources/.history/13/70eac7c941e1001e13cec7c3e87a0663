#include <stdio.h>

struct info
{
  char name [20];
  int roll;
  float marks;
};


int main()
{
    struct info information = {"Ahmed Mohamed", 2, 95.5};
    printf("Name is: %s\n", information.name);
    printf("Roll is: %d\n", information.roll);
    printf("Name is: %0.2f\n", information.marks);
    return 0;
}

